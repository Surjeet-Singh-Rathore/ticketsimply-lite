<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    xmlns:shimmer="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/flash_white_bg"
    android:orientation="vertical">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/layoutCancelTicketSheet"
        android:name="com.bitla.ts.app.base.CancelTicketSheet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_16"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.fragment.app.FragmentContainerView>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/layoutEditPassengerSheet"
        android:name="com.bitla.ts.app.base.EditPassengerSheet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_16"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.fragment.app.FragmentContainerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_date_details"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginEnd="@dimen/dp_16"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0"
        android:layout_marginBottom="@dimen/dp_56"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rv_date_details">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.facebook.shimmer.ShimmerFrameLayout
                android:id="@+id/shimmerBookTicket"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/dp_8"
                android:layout_marginEnd="@dimen/dp_8"
                android:orientation="vertical"
                android:visibility="gone"
                shimmer:duration="800">

                <include layout="@layout/shimmer_book_ticket" />

            </com.facebook.shimmer.ShimmerFrameLayout>

            <LinearLayout
                android:id="@+id/myBookingBookTicketContainer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/button_height"
                    android:layout_marginTop="10dp"
                    android:elevation="4dp"
                    android:orientation="vertical"
                    android:visibility="gone"
                    app:cardBackgroundColor="@color/colorWhite"
                    card_view:cardCornerRadius="4dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@color/colorWhite"
                        android:orientation="horizontal"
                        android:paddingLeft="20dp"
                        android:paddingRight="20dp">

                        <ImageView
                            android:id="@+id/search"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentStart="true"
                            android:layout_centerInParent="true"
                            android:background="?selectableItemBackgroundBorderless"
                            android:src="@drawable/ic_search"
                            app:tint="@color/colorBlackShadow" />

                        <EditText
                            android:id="@+id/autoCompleteTextView"
                            style="@style/textInputStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_toStartOf="@id/text_cancel"
                            android:layout_toEndOf="@id/search"
                            android:background="@null"
                            android:ems="10"
                            android:hint="@string/pnr_name_mobile_number"
                            android:text=""
                            android:textColor="@color/colorDimShadow6" />

                        <TextView
                            android:id="@+id/text_cancel"
                            style="@style/TextStyleBold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerInParent="true"
                            android:gravity="right|center"
                            android:text="@string/cancel"
                            android:textColor="@color/colorRed"
                            android:textSize="18sp" />

                        <android.widget.Button
                            android:id="@+id/btn_pnr_Search"
                            style="@style/TextStyleBold"
                            android:layout_width="64dp"
                            android:layout_height="32dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="20dp"
                            android:background="@drawable/white_bg"
                            android:gravity="center"
                            android:text="@string/search"
                            android:textAllCaps="false"
                            android:textColor="@color/colorWhite"
                            android:textSize="11sp"
                            app:backgroundTint="@color/colorPrimary" />

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_booking_src_des"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <LinearLayout
                    android:id="@+id/layout_home_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/flash_white_bg"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">


                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp"
                            android:elevation="5dp"
                            android:orientation="vertical"
                            app:cardBackgroundColor="@color/colorWhite"
                            card_view:cardCornerRadius="5dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <FrameLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginStart="5dp"
                                                android:layout_marginTop="10dp"
                                                android:layout_marginEnd="5dp"
                                                android:orientation="vertical">

                                                <com.google.android.material.textfield.TextInputLayout
                                                    android:id="@+id/source_dropdown"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="@dimen/text_input_edit_text_height"
                                                    android:background="@drawable/white_bg"
                                                    android:drawableLeft="@drawable/ic_destination"
                                                    android:hint="@string/enterSource"
                                                    app:boxBackgroundColor="@color/white"
                                                    app:boxBackgroundMode="filled"
                                                    app:boxStrokeErrorColor="@color/colorRed2"
                                                    app:boxStrokeWidth="@dimen/dp_2"
                                                    app:errorEnabled="true"
                                                    app:errorIconDrawable="@null"
                                                    app:layout_constraintStart_toStartOf="parent"
                                                    app:layout_constraintTop_toBottomOf="@+id/titleTV5">


                                                    <com.google.android.material.textfield.TextInputEditText
                                                        android:id="@+id/source_dropdown_tv"
                                                        style="@style/textInputStyle"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="wrap_content"
                                                        android:focusable="false"
                                                        android:singleLine="true"
                                                        android:focusableInTouchMode="false"
                                                        android:imeOptions="actionNext"
                                                        android:inputType="none"

                                                        app:endIconMode="custom" />

                                                </com.google.android.material.textfield.TextInputLayout>


                                                <TextView
                                                    android:id="@+id/tvSource"
                                                    style="@style/msg_context"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginTop="14dp"
                                                    android:layout_marginBottom="14dp"
                                                    android:drawableLeft="@drawable/ic_source"
                                                    android:drawablePadding="5dp"
                                                    android:drawableTint="@color/colorText"
                                                    android:gravity="center|left"
                                                    android:hint="@string/enterSource"
                                                    android:text="@string/empty"
                                                    android:textColor="@color/colorBlackShadow" />
                                            </LinearLayout>

                                            <View
                                                android:layout_width="match_parent"
                                                android:layout_height="1dp"
                                                android:layout_marginTop="5dp"
                                                android:background="@color/colorDimShadow1" />

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginStart="5dp"
                                                android:layout_marginEnd="5dp"
                                                android:orientation="vertical">

                                                <TextView
                                                    android:id="@+id/tvDestination"
                                                    style="@style/msg_context"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginTop="14dp"
                                                    android:layout_marginBottom="10dp"
                                                    android:drawableLeft="@drawable/ic_destination"
                                                    android:drawablePadding="5dp"
                                                    android:gravity="center|left"
                                                    android:hint="@string/enterDestination"

                                                    android:text="@string/empty"
                                                    android:textColor="@color/colorBlackShadow" />

                                                <com.google.android.material.textfield.TextInputLayout
                                                    android:id="@+id/destination_dropdown"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="@dimen/text_input_edit_text_height"
                                                    android:layout_marginTop="8dp"
                                                    android:background="@drawable/white_bg"
                                                    android:drawableLeft="@drawable/ic_destination"
                                                    android:hint="@string/enterDestination"
                                                    app:boxBackgroundColor="@color/white"
                                                    app:boxBackgroundMode="filled"
                                                    app:boxStrokeErrorColor="@color/colorRed2"
                                                    app:boxStrokeWidth="@dimen/dp_2"
                                                    app:errorEnabled="true"
                                                    app:errorIconDrawable="@null"
                                                    app:layout_constraintStart_toStartOf="parent"
                                                    app:layout_constraintTop_toBottomOf="@+id/titleTV5">


                                                    <androidx.appcompat.widget.AppCompatEditText
                                                        android:id="@+id/destination_dropdown_tv"
                                                        style="@style/textInputStyle"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="wrap_content"
                                                        android:focusable="false"
                                                        android:focusableInTouchMode="false"
                                                        android:singleLine="true"
                                                        android:imeOptions="actionNext"
                                                        android:inputType="none"
                                                        app:endIconMode="custom" />

                                                </com.google.android.material.textfield.TextInputLayout>

                                            </LinearLayout>


                                            <View
                                                android:layout_width="match_parent"
                                                android:layout_height="1dp"
                                                android:layout_marginTop="5dp"
                                                android:background="@color/colorDimShadow1" />


                                        </LinearLayout>

                                        <FrameLayout
                                            android:id="@+id/btnRotate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="right"
                                            android:layout_marginTop="44dp"
                                            android:layout_marginRight="10dp"
                                            android:background="@drawable/circle_shape"
                                            android:backgroundTint="@color/colorText"
                                            android:elevation="5dp"
                                            android:visibility="gone">

                                            <ImageView
                                                android:id="@+id/imgRotate"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:visibility="visible"
                                                app:srcCompat="@drawable/ic_flip" />

                                        </FrameLayout>

                                        <FrameLayout
                                            android:id="@+id/btnRotateHumsafar"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="end"
                                            android:layout_marginTop="@dimen/_38sdp"
                                            android:layout_marginEnd="10dp"
                                            android:background="@drawable/circle_shape"
                                            android:backgroundTint="@color/colorText"
                                            android:elevation="5dp"
                                            android:visibility="visible">

                                            <ImageView
                                                android:id="@+id/imgRotateHumsafar"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:visibility="visible"
                                                app:srcCompat="@drawable/ic_flip" />

                                        </FrameLayout>

                                    </FrameLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="5dp"
                                        android:layout_marginTop="14dp"
                                        android:layout_marginEnd="5dp"
                                        android:layout_marginBottom="10dp"
                                        android:orientation="horizontal"
                                        android:weightSum="1">

                                        <TextView
                                            android:id="@+id/tvSelectDate"
                                            style="@style/msg_context"
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.50"
                                            android:drawableLeft="@drawable/ic_calendar_start"
                                            android:drawablePadding="5dp"
                                            android:gravity="center|left"
                                            android:hint="@string/departure_date"
                                            android:text="@string/empty"
                                            android:textColor="@color/colorBlackShadow" />

                                        <TextView
                                            android:id="@+id/tvTodayDate"
                                            style="@style/TextStyleBold"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="0.25"
                                            android:background="?selectableItemBackgroundBorderless"
                                            android:gravity="center"
                                            android:text="@string/today"
                                            android:textColor="@color/button_default_color"
                                            android:textSize="12sp" />


                                        <TextView
                                            android:id="@+id/tvTomorrowDate"
                                            style="@style/TextStyleBold"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="0.25"
                                            android:background="?selectableItemBackgroundBorderless"
                                            android:gravity="center"
                                            android:text="@string/tomorrow"
                                            android:singleLine="true"
                                            android:textColor="@color/button_default_color"
                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_marginTop="5dp"
                                        android:layout_marginBottom="10dp"
                                        android:background="@color/colorDimShadow1" />

                                    <LinearLayout
                                        android:id="@+id/layout_return"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical"
                                        android:visibility="gone">

                                        <RelativeLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="14dp"
                                            android:layout_marginBottom="14dp">

                                            <TextView
                                                android:id="@+id/tvSelectReturnDate"
                                                style="@style/msg_context"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentLeft="true"
                                                android:drawableLeft="@drawable/ic_calendar_stop"
                                                android:drawablePadding="5dp"
                                                android:gravity="center|left"
                                                android:hint="@string/return_date"
                                                android:text="@string/empty"
                                                android:textColor="@color/colorBlackShadow" />


                                            <TextView
                                                android:id="@+id/tvOptional"
                                                style="@style/TextStyleBold"
                                                android:layout_width="wrap_content"
                                                android:layout_height="match_parent"
                                                android:layout_alignParentRight="true"
                                                android:layout_centerInParent="true"
                                                android:gravity="center"
                                                android:text="@string/optional"
                                                android:textColor="@color/button_default_color"
                                                android:textSize="12sp" />

                                            <ImageView
                                                android:id="@+id/img_clear"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:layout_centerInParent="true"
                                                android:src="@drawable/ic_cross"
                                                android:visibility="gone" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="1dp"
                                            android:layout_marginTop="5dp"
                                            android:background="@color/colorDimShadow1"
                                            android:visibility="gone" />
                                    </LinearLayout>
                                </LinearLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:id="@+id/selectServiceCL"
                                    android:layout_marginEnd="5dp"
                                    android:visibility="visible"
                                    android:orientation="vertical">




                                    <LinearLayout
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintStart_toEndOf="parent"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        android:weightSum="6">

                                        <TextView
                                            android:id="@+id/bus_services"
                                            style="@style/msg_context"
                                            android:layout_width="0dp"
                                            android:visibility="gone"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="14dp"
                                            android:layout_marginBottom="14dp"
                                            android:layout_weight="5"
                                            android:drawableLeft="@drawable/ic_source"
                                            android:drawablePadding="5dp"
                                            android:drawableTint="@color/colorText"
                                            android:gravity="center|left"
                                            android:hint="@string/select_service"
                                            android:text="@string/empty"
                                            android:textColor="@color/colorBlackShadow" />
                                        <com.google.android.material.textfield.TextInputLayout
                                            android:id="@+id/timeInMins3"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/text_input_edit_text_height"
                                            android:drawableLeft="@drawable/ic_destination"
                                            android:background="@drawable/white_bg"
                                            app:boxStrokeErrorColor="@color/colorRed2"
                                            app:boxStrokeWidth="@dimen/dp_2"
                                            app:errorEnabled="true"
                                            app:errorIconDrawable="@null"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintTop_toBottomOf="@+id/titleTV5"
                                            android:hint="@string/select_service"
                                            app:boxBackgroundColor="@color/white"
                                            app:boxBackgroundMode="filled">


                                            <androidx.appcompat.widget.AppCompatEditText
                                                android:inputType="none"
                                                android:id="@+id/tvService"
                                                style="@style/textInputStyle"
                                                android:focusable="false"
                                                android:focusableInTouchMode="false"
                                                android:singleLine="true"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:imeOptions="actionNext"
                                                app:endIconMode="custom" />

                                        </com.google.android.material.textfield.TextInputLayout>

                                        <ImageView
                                            android:id="@+id/bus_service_cross"
                                            android:layout_width="@dimen/dp_0"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center"
                                            android:layout_weight="1"
                                            android:paddingTop="@dimen/dp_8"
                                            android:paddingBottom="@dimen/dp_8"
                                            android:src="@drawable/ic_cross" />
                                    </LinearLayout>

                                    <androidx.appcompat.widget.AppCompatImageView
                                        android:layout_width="@dimen/_30sdp"
                                        android:id="@+id/clearIV"
                                        android:visibility="gone"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        android:layout_marginBottom="@dimen/_3sdp"
                                        android:layout_marginEnd="@dimen/_5sdp"
                                        android:padding="@dimen/_10sdp"
                                        android:tint="@color/light_gray"
                                        android:src="@drawable/ic_cancel_black"
                                        android:layout_height="@dimen/_30sdp"/>


                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        app:layout_constraintTop_toTopOf="parent"
                                        android:layout_marginTop="5dp"
                                        android:background="@color/colorDimShadow1"
                                        android:visibility="gone" />
                                </androidx.constraintlayout.widget.ConstraintLayout>


                                <ProgressBar
                                    android:id="@+id/progress"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:visibility="gone" />

                                <android.widget.Button
                                    android:id="@+id/btnSearch"
                                    style="@style/msg_context"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/button_height"
                                    android:layout_marginTop="4dp"
                                    android:background="@drawable/button_selected_bg"
                                    android:text="@string/search"
                                    android:textAllCaps="false"
                                    android:textColor="@color/colorWhite" />

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/tvLastBookedTitle"
                    style="@style/TextStyleBold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:gravity="start"
                    android:text="@string/most_frequently_booked"
                    android:textColor="@color/colorBlackShadow"
                    android:visibility="visible" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rvLastBooked"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp" />
            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/busDetailsContainer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <include
                    android:id="@+id/busDetailsIncludeLayout"
                    layout="@layout/layout_bus_details_book_ticket"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <ImageView
                android:id="@+id/noData"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp_100"
                android:src="@drawable/ic_no_search_results"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvNoService"
                style="@style/TextStyleNormal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:letterSpacing="0.05"
                android:paddingStart="@dimen/dp_4"
                android:paddingEnd="@dimen/dp_4"
                android:text="@string/not_authorized_to_access"
                android:textColor="@color/grey"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/noData" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

    <include android:id="@+id/includeProgress"
        layout="@layout/layout_progress_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
     />


